@model VendorDomainObj

@{
    ViewData["Title"] = "Vendor Details";
}

<h2>Vendor Details</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>ID</dt>
        <dd>
            @Html.DisplayFor(model => model.BusinessEntityID)
        </dd>
        <dt>Account Number</dt>
        <dd>
            @Html.DisplayFor(model => model.AccountNumber)
        </dd>
        <dt>Name</dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt>Credit Rating</dt>
        <dd>            
            @Html.DisplayFor(model => model.CreditRating)
        </dd>
        <dt>
            Preferred Vendor?
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PreferredVendor)
        </dd>
        <dt>
            Active?
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IsActive)
        </dd>
        <dt>
            Purchasing Web Service URL
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PurchasingWebServiceURL)
        </dd>        
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.BusinessEntityID">Edit</a> |
    <a asp-action="List">Back to List</a>
</div>
